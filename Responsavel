create TABLE if NOT EXISTS responsavel(
  id Responavel INTEGER PRIMARY KEY AUTOINCREMENT,
  nome VARCHAR (100)not NULL,
  grau_parentesco TEXT,
  telefone VARCHAR (11)NOT NULL UNIQUE,
  cpf VARCHAR (16)NOT NULL UNIQUE,
  email VARCHAR (50),
  cgm
);

