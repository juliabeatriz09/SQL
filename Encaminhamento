create TABLE if NOT EXISTS encaminhamento(
  id_encaminhamento INTEGER PRIMARY KEY AUTOINCREMENT,
  tipo_fo TEXT not NULL ,
  justificativa TEXT not NULL,
  providencia TEXT,
  data DATE not NULL,
  cmg ,
  funcionario
);

